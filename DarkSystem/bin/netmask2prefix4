#!/bin/sh

netmask2prefix4(){
	case "${1}" in
		'0.0.0.0'		) echo '0';;
		'128.0.0.0'		) echo '1';;
		'192.0.0.0'		) echo '2';;
		'224.0.0.0'		) echo '3';;
		'240.0.0.0'		) echo '4';;
		'248.0.0.0'		) echo '5';;
		'252.0.0.0'		) echo '6';;
		'254.0.0.0'		) echo '7';;
		'255.0.0.0'		) echo '8';;
		'255.128.0.0'		) echo '9';;
		'255.192.0.0'		) echo '10';;
		'255.224.0.0'		) echo '11';;
		'255.240.0.0'		) echo '12';;
		'255.248.0.0'		) echo '13';;
		'255.252.0.0'		) echo '14';;
		'255.254.0.0'		) echo '15';;
		'255.255.0.0'		) echo '16';;
		'255.255.128.0'		) echo '17';;
		'255.255.192.0'		) echo '18';;
		'255.255.224.0'		) echo '19';;
		'255.255.240.0'		) echo '20';;
		'255.255.248.0'		) echo '21';;
		'255.255.252.0'		) echo '22';;
		'255.255.254.0'		) echo '23';;
		'255.255.255.0'		) echo '24';;
		'255.255.255.128'	) echo '25';;
		'255.255.255.192'	) echo '26';;
		'255.255.255.224'	) echo '27';;
		'255.255.255.240'	) echo '28';;
		'255.255.255.248'	) echo '29';;
		'255.255.255.252'	) echo '30';;
		'255.255.255.254'	) echo '31';;
		'255.255.255.255'	) echo '32';;
		*			) echo '32';;
	esac
	return 0;

}

netmask2prefix4 ${1}

