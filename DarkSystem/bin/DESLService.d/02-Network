main(){
	${BIN_FOLDER:-/DRoot/DarkSystem/bin}/NetConfig
	R=${?};
	[ ! "${R}" = '0' ] && echo 'WARNING' >&9
	sleep 1

	exit ${R};
}

main "${@}";

exit 0;

